<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
		http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<profiles>
		<profile>
			<id>with-zephyrsoft-mirror</id>
			<mirrors>
				<mirror>
					<id>nexus</id>
					<mirrorOf>*</mirrorOf>
					<url>https://dev.zephyrsoft.org/nexus/content/groups/public</url>
				</mirror>
			</mirrors>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>with-zephyrsoft-mirror</activeProfile>
	</activeProfiles>

</settings>